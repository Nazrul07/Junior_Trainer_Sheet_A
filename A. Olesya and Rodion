#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,t,i;
    cin>>n>>t;
    if(t==10 && n>1)
    {
        for(i=1 ; i<=n-1 ; i++)
        {
            cout<<"1";
        }
        cout<<"0";
    }
    else if(t==10 && n==1)cout<<"-1";
    else
    {
        for(i=0 ; i<n ; i++)
        {
            cout<<t;
        }
    }
    return 0;
}
