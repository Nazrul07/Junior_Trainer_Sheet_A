#include<iostream>
using namespace std;
int main()
{
    int t,a,b,c,Count=0;
    cin>>t;
    for(int i=0 ; i<t ; i++)
    {
        cin>>a>>b>>c;
        if((a==1 && b==1 && c==1) || (a==1 && b==1) || (a==1 && c==1) || (b==1 && c==1))
            Count++;
    }
    cout<<Count<<endl;
    return 0;
}
