#include <iostream>
using namespace std;
int main() {
    int t,count=0,out=0;
    cin>>t;
    int a[t];
    for(int i=0 ; i<t ; i++)
    {
        cin>>a[i];
        if(a[i]>0)
        {
            count=count+a[i];
        }
        else
        {
            count=count+a[i];
            if(count<0)
            {
                out=out+abs(count);
                count=0;
            }
        }
    }
    cout<<out<<endl;
    return 0;
}
