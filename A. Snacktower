#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n;
    cin>>t;
    n=t;
    int a[t+1]={0};
    for(int i=0 ; i<t ; i++)
    {
        int x;
        cin>>x;
        a[x]=1;
        for( ; a[n]==1 ; n--)
        {
            cout<<n<<" ";
        }
        cout<<endl;
    }
    return 0;
}
