#include <iostream>
using namespace std;
int main() {
    long long m,n;
    cin>>m>>n;
    if(n-m>=2 && m%2==0)
    {
        cout<<m<<" "<<m+1<<" "<<m+2;
    }
    else if(n-m>=3 && m%2==1)
    {
        cout<<m+1<<" "<<m+2<<" "<<m+3;
    }
    else
    {
        cout<<"-1";
    }
    return 0;
}
