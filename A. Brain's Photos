#include<bits/stdc++.h>
using namespace std;
int main()
{
    int m,n,i,j,k=0;
    cin>>m>>n;
    char a[m][n];
    for(i=0 ; i<m ; i++)
    {
        for(j=0 ; j<n ; j++)
        {
            cin>>a[i][j];
            if(a[i][j]=='C' || a[i][j]=='M' || a[i][j]=='Y')
            {
                k=1;
            }
        }
    }
    if(k==1)cout<<"#Color"<<endl;
    else cout<<"#Black&White"<<endl;
    return 0;
}
